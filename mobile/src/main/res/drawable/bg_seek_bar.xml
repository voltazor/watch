<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>

        <shape android:shape="ring"
               android:useLevel="false"
               android:innerRadius="55dp"
               android:thickness="200dp">

            <size
                android:width="200dp"
                android:height="200dp"/>

            <gradient
                android:type="sweep"
                android:startColor="@color/blue_pressed"
                android:endColor="@color/green_pressed"/>

        </shape>

    </item>

    <item>

        <shape android:shape="ring"
               android:useLevel="false"
               android:innerRadius="70dp"
               android:thickness="200dp">

            <stroke
                android:color="@android:color/white"
                android:width="30dp"
                android:dashGap="2dp"
                android:dashWidth="3dp"/>

            <size
                android:width="200dp"
                android:height="200dp"/>

        </shape>

    </item>

    <item>

        <shape android:shape="ring"
               android:innerRadius="0dp"
               android:thickness="55dp"
               android:useLevel="false">

            <solid android:color="@android:color/white"/>

        </shape>

    </item>

    <item>

        <shape android:shape="ring"
               android:innerRadius="85dp"
               android:thickness="100dp"
               android:useLevel="false">

            <solid android:color="@android:color/white"/>

        </shape>

    </item>

</layer-list>